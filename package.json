{
  "name": "meanfi-ui",
  "version": "1.28.14",
  "private": true,
  "browserslist": {
    "production": [
      "chrome >= 67",
      "edge >= 79",
      "firefox >= 68",
      "opera >= 54",
      "safari >= 14"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/supermean-org/meanfi-ui"
  },
  "homepage": "/",
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!antd|@ant-design|rc-.+?|@babel/runtime).+(js|jsx)$"
    ]
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "start:local": "NODE_ENV=local REACT_APP_ENV=local npm start",
    "start:local-validator": "REACT_APP_ENV=local-validator npm start",
    "start:local:win": "set 'NODE_ENV=local' && set 'REACT_APP_ENV=local' && npm start",
    "start:dev": "NODE_ENV=development REACT_APP_ENV=development npm start",
    "start:dev:win": "set 'NODE_ENV=development' && set 'REACT_APP_ENV=development' && npm start",
    "start:prod": "NODE_ENV=production REACT_APP_ENV=production npm start",
    "start:prod:win": "set 'NODE_ENV=production' && set 'REACT_APP_ENV=production' && npm start",
    "start:staging": "NODE_ENV=staging REACT_APP_ENV=staging npm start",
    "start:staging:win": "set 'NODE_ENV=staging' && set 'REACT_APP_ENV=staging' && npm start",
    "build:dev": "NODE_ENV=development REACT_APP_ENV=development npm run build",
    "build:dev:win": "set 'NODE_ENV=development' && set 'REACT_APP_ENV=development' && npm run build",
    "build:staging": "NODE_ENV=staging REACT_APP_ENV=staging npm run build",
    "build:staging:win": "set 'NODE_ENV=staging' && set 'REACT_APP_ENV=staging' && npm run build",
    "build:prod": "NODE_ENV=production REACT_APP_ENV=production npm run build",
    "build:prod:win": "set 'NODE_ENV=production' && set 'REACT_APP_ENV=production' && npm run build",
    "postbuild": "node ci/copy-config.js",
    "lint": "biome check --apply .",
    "build:s3": "NODE_OPTIONS=\"--max_old_space_size=4096\" react-app-rewired build"
  },
