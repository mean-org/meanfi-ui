{
  "name": "meanfi-ui",
  "version": "0.8.8",
  "private": true,
  "scripts": {
    "clean": "rm -rf build/*",
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "start:local": "REACT_APP_ENV=local npm start",
    "start:local:win": "set 'REACT_APP_ENV=local' && npm start",
    "start:dev": "REACT_APP_ENV=development npm start",
    "start:dev:win": "set \"REACT_APP_ENV=development\" && npm start",
    "start:prod": "REACT_APP_ENV=production npm start",
    "start:prod:win": "set \"REACT_APP_ENV=production\" && npm start",
    "start:staging": "REACT_APP_ENV=staging npm start",
    "start:staging:win": "set \"REACT_APP_ENV=staging\" && npm start",
    "build:dev": "REACT_APP_ENV=development npm run build",
    "build:dev:win": "set \"REACT_APP_ENV=development\" && npm run build",
    "build:staging": "REACT_APP_ENV=staging npm run build",
    "build:staging:win": "set \"REACT_APP_ENV=staging\" && npm run build",
    "build:prod": "REACT_APP_ENV=production npm run build",
    "build:prod:win": "set \"REACT_APP_ENV=production\" && npm run build",
    "postbuild": "node ci/copy-config.js",
    "format:fix": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|css|md)\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mean-dao/meanfi-ui"
  },
  "homepage": "/",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
